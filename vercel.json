{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  {
   "rewrites":  [
       {"source": "/(.*)", "destination": "/"}
    ]
  }
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(.*).(js|css|jpg|png|svg|woff|woff2|ttf)",
      "dest": "/dist/$1.$2"
    }
  ]
}
